{
  "presets": [
    [
      "@babel/preset-env", {
        "modules": "auto",
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ],
    "@vue/babel-preset-jsx"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "development":{
      "plugins": ["dynamic-import-node-babel-7"]
    },
    "test":{
      "plugins": ["dynamic-import-node-babel-7"]
    }
  }
}
